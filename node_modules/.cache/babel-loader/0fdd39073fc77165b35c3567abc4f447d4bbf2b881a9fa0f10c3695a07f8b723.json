{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'toList',\n  data() {\n    return {\n      lista: {\n        checked: false,\n        text: '',\n        id: 0,\n        todo: ''\n      },\n      listas: []\n    };\n  },\n  methods: {\n    addLista() {\n      this.listas.push(this.lista); // add um item na lista\n      this.lista = {\n        checked: false,\n        text: ''\n      };\n    },\n    changeChecked(index) {\n      this.listas[index].checked = !this.listas[index].checked;\n      console.log(this.listas[index].checked);\n    },\n    deleta() {\n      this.listas.pop(this.listas);\n      console.log('veio parar aqui');\n    },\n    add(todo) {\n      if (todo.trim().length) {\n        this.todos.push({\n          id: this.genId(),\n          //gera um novo id\n          name: todo,\n          done: false\n        });\n        this.todo = \"\";\n      } else {\n        alert(\"Por favor, entre com caracteres validos\");\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAyBA;EACAA;EACAC;IACA;MACAC;QACAC;QACAC;QACAC;QACAC;MACA;MAEAC;IACA;EACA;EACAC;IACAC;MACA;MACA;QACAN;QACAC;MACA;IACA;IACAM;MACA;MACAC;IACA;IACAC;MAEA;MACAD;IAEA;IACAE;MACA;QACA;UACAR;UAAA;UACAL;UACAc;QACA;QACA;MACA;QACAC;MACA;IAEA;EAIA;AAEA","names":["name","data","lista","checked","text","id","todo","listas","methods","addLista","changeChecked","console","deleta","add","done","alert"],"sourceRoot":"src/components","sources":["toList.vue"],"sourcesContent":["<template>\n  <div class=\"conteiner\">\n    <form >\n      <h3>Digite um item</h3>\n      <input type=\"text\" @keyup.enter=\"add(todo)\" id=\"todo\"  v-model.trim=\"lista.text\"  placeholder=\"Digite Qualque coisa\">\n        <button type=\"button\" @click=\"addLista()\" > Enviar</button>\n\n   \n\n        <div v-for=\"(lista, index) in listas\" :key=\"index\" class=\"list\">\n          <form class=\"mr-10\" >\n            <input :id=\"index\" type=\"checkbox\" v-model.trim=\"lista.checked\" @click=\"changeChecked(index)\" />\n          </form>\n\n          <span :class=\"listas[index].checked ? 'line-through mr-10' : 'mr-10'\" >{{ lista.text }}</span>\n\n          <a class=\"mr-10\" href=\"#\" @click=\"deleta\">Excluir</a>\n        </div>\n    </form>\n    <br>\n  </div>\n    \n</template>\n<script>\n\nexport default {\n  name:'toList', \n  data(){\n    return{\n      lista: {\n        checked: false,\n        text: '',\n        id:0,\n        todo:'',\n      },\n\n      listas: [],\n    }\n  },\n  methods:{\n    addLista(){\n      this.listas.push(this.lista) // add um item na lista\n      this.lista = {\n        checked: false,\n        text: '',\n      }          \n    },\n    changeChecked (index) {\n      this.listas[index].checked = !this.listas[index].checked\n      console.log(this.listas[index].checked) \n    },\n    deleta(){\n      \n      this.listas.pop(this.listas)\n      console.log('veio parar aqui');                                                            \n  \n    },\n    add(todo) {\n        if (todo.trim().length) {\n            this.todos.push({\n              id: this.genId(), //gera um novo id\n              name: todo,\n              done: false, \n            });\n            this.todo = \"\"; \n        } else {\n            alert(\"Por favor, entre com caracteres validos\")\n        }\n     \n    },\n\n    \n     \n  }\n \n}\n</script>\n\n<style>\n  body{\n    background: #4fc08c;\n    margin: 5px;\n  }\n  .conteiner{\n    background: white;\n    max-height: 500px;\n    padding-top: 15px;  \n    text-align: center; \n    border-radius: 15px;\n   \n  }\n \n  button{\n    border-radius: 50px;\n    background: #4fc08c;\n    width: 60px;\n    margin: 5px;\n\n  }\n\n  li{\n    padding-top: 10px;\n  }\n  a{\n    color: red;\n  }\n\n  .line-through {\n    text-decoration: line-through;\n    color: rgba(0, 0, 0, 0.3);\n  }\n\n  .list {\n    display: flex;\n    justify-content: justify;\n    padding-left: 160px;\n    margin-top: 15px;\n    margin-bottom: 15px;\n  }\n\n  .mr-10 {\n    margin-right: 10px;\n  }\n  \n  \n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}